{
  "name": "darkin1/intercom",
  "description": "Wrapper on the Intercom class provided by Intercom - with support for Laravel 5.x, 6.x, 7.x",
  "keywords": ["laravel", "intercom", "intercom.io", "api", "guzzle"],
  "license": "MIT",
  "authors": [{
    "name": "Dariusz Ciesielski",
    "email": "dciesielski87@gmail.com"
  }],
  "require": {
    "php": ">= 7.1",
    "intercom/intercom-php": "^4.0",
    "illuminate/support": "^5.8|^6.0|^7.0",
    "php-http/guzzle6-adapter": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Darkin1\\Intercom\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Darkin1\\Intercom\\IntercomServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev"
}
